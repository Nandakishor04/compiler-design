#include <stdio.h>
#include <string.h>

char input[20];
int i = 0;

void E();
void E_dash();

void match(char c) {
    if (input[i] == c)
        i++;
    else {
        printf("Error\n");
    }
}

void E() {
    if (input[i] == 'i') {
        match('i');
        E_dash();
    } else
        printf("Error\n");
}

void E_dash() {
    if (input[i] == '+') {
        match('+');
        match('i');
        E_dash();
    }
    // else epsilon (do nothing)
}

int main() {
    printf("Enter string: ");
    scanf("%s", input);

    E();

    if (input[i] == '\0')
        printf("String Accepted\n");
    else
        printf("String Rejected\n");

    return 0;
}
